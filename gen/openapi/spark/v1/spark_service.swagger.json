        "commit": {
          "type": "string"
        },
        "buildTime": {
          "type": "string"
        }
      }
    },
    "v1RunJobRequest": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "title": "name of the job"
        },
        "namespace": {
          "type": "string",
          "title": "namespace of the job"
        },
        "image": {
          "type": "string",
          "title": "image of the job"
        },
        "command": {
          "type": "string",
          "title": "command of the job"
        },
        "timeout": {
          "type": "integer",
          "format": "int32",
          "title": "timeout of the job"
        }
      }
